inherit distutils

NAME="python-pyparsing"
VERSION=1.5.7
RELEASE=1
CATEGORY="Python"
SUMMARY="Python grammar parsing module"
DESCRIPTION="pyparsing is a general parsing module for Python. Grammars are
implemented directly in the client code using parsing objects, instead of
externally, as with lex/yacc-type tools."
HOMEPAGE="http://pyparsing.wikispaces.com/"
SRC_URI="mirror://sourceforge/pyparsing/pyparsing-${VERSION}.tar.gz"

DOCS="HowToUsePyparsing.html"
